<h1>Listing notes</h1>

<table>
  <tr>
    <th>Notable</th>
    <th>Notable type</th>
    <th>Content</th>
    <th>Startdate</th>
    <th>Enddate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.notable_id %></td>
    <td><%= note.notable_type %></td>
    <td><%= note.content %></td>
    <td><%= note.startDate %></td>
    <td><%= note.endDate %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Note', new_note_path %>
