<h1>Listing employee_psas</h1>

<table>
  <tr>
    <th>Startdate</th>
    <th>Enddate</th>
    <th>Activeboolean</th>
    <th>Employee</th>
    <th>Psa</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @employee_psas.each do |employee_psa| %>
  <tr>
    <td><%= employee_psa.startDate %></td>
    <td><%= employee_psa.endDate %></td>
    <td><%= employee_psa.activeBoolean %></td>
    <td><%= employee_psa.employee %></td>
    <td><%= employee_psa.psa %></td>
    <td><%= link_to 'Show', employee_psa %></td>
    <td><%= link_to 'Edit', edit_employee_psa_path(employee_psa) %></td>
    <td><%= link_to 'Destroy', employee_psa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

